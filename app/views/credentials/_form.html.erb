
<div id="body">
<div id="body-left">
<%= form_for(@credential) do |f|%>
  <%= f.label :Username %>
  <%= f.text_field :username %><br/>
  <%= f.label :mail %>
  <%= f.text_field :mail %><br/>
<%= f.label :Password  %>
  <%= f.password_field :password %><br />
  <%= f.submit "Login" %><br /></br />
<% end %>

<%= link_to "Visit Other Site", "http://www.rubyonrails.org/", :confirm => "Are you sure?" %>
